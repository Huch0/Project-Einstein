# ===== Core Infrastructure =====
APP_ENV=dev
PORT=8000
DATABASE_URL=postgresql+psycopg://app:app@localhost:5432/app
REDIS_URL=redis://localhost:6379/0
S3_ENDPOINT_URL=http://localhost:9000
S3_ACCESS_KEY_ID=minio
S3_SECRET_ACCESS_KEY=minio123
S3_BUCKET=sim-uploads
JWT_SECRET=change-me

# ===== OpenAI API (v0.4) =====
OPENAI_API_KEY=sk-...
OPENAI_MODEL=gpt-5  # gpt-5, gpt-4o, etc.
OPENAI_TEMPERATURE=1.0
OPENAI_MAX_OUTPUT_TOKENS=4096

# ===== Unified Chat (v0.4) =====
CHAT_DEFAULT_MODE=ask  # "ask" | "agent"
CHAT_ENABLE_STREAMING=true
CHAT_MAX_CONVERSATION_LENGTH=50
CHAT_CONVERSATION_TIMEOUT_S=3600

# ===== Universal Physics Builder (v0.4) =====
BUILDER_MODE=universal
BUILDER_DEFAULT_SCALE_M_PER_PX=0.01
BUILDER_DEFAULT_GRAVITY_M_S2=9.81
BUILDER_INFER_CONSTRAINTS=true

# ===== Physics Engine (v0.4) =====
PHYSICS_ENGINE=matter-js  # Analytic solver removed in v0.4
PHYSICS_TIME_STEP_S=0.016  # 60 fps
PHYSICS_DEFAULT_DURATION_S=5.0
PHYSICS_MAX_BODIES=100

# ===== Agent Tools =====
SAM_HTTP_URL=http://localhost:9001/segment
SAM_TIMEOUT_S=10.0

# Labeler (GPT Vision)
LABELER_MODE=openai
LABELER_MODEL=gpt-5

MATTER_WORKER_PATH=  # Leave empty to use default path
MATTER_WORKER_TIMEOUT_S=10.0

# ===== Frontend & CORS =====
FRONTEND_ORIGIN=http://localhost:9002

# ===== Logging & Tracing =====
LANGCHAIN_API_KEY=
LANGCHAIN_TRACING_V2=false
